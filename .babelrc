{
  "presets": [
    ["@babel/preset-env",
      {
        "targets": {
          "node": "current",
          "browsers": [
            ">0.25%",
            "not ie 11",
            "not op_mini all"
          ]
        }
      }],
    "@babel/preset-react"
  ],
  "plugins": [
    "syntax-async-functions",
    "@babel/plugin-transform-async-to-generator"
  ]
}